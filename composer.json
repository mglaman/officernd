{
    "name": "mglaman/officernd",
    "description": "SDK for integrating with the OfficeR&D API",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Glaman",
            "email": "nmd.matt@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "mglaman\\OfficeRND\\": "src"
        }
      },
      "autoload-dev": {
          "psr-4": {
              "mglaman\\OfficeRND\\Tests\\": "tests"
          }
      },
      "scripts": {
          "test": "./vendor/bin/phpunit -c phpunit.xml.dist",
          "cs": "./vendor/bin/phpcs --standard=psr2 src/"
      }
}
